  "scripts": {
    "start": "cli-tasks start",
    "build": "cli-tasks build",
    "test": "cli-tasks test:ng",
    "lint": "cli-tasks lint",
    "e2e": "cli-tasks test:e2e"
  },
  "tasks": {
    "build": [
      "[clean]",
      "ng build --prod"
    ],
    "clean": [
      "rimraf dist"
    ],
    "lint": [
      "ng lint"
    ],
    "start": [
      "ng serve"
    ],
    "start:dev": [
      "ng serve -o"
    ],
    "test": [
      "[test:ng]",
      "[test:e2e]"
    ],
    "test:ng": [
      "ng test"
    ],
    "test:e2e": [
      "ng e2e"
    ]
  }
