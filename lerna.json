{
  "commands": {
    "exec": {
      "bail": true
    },
    "publish": {
      "allowBranch": "master",
      "ignore": [
        "*.md",
        "LICENSE"
      ],
      "skipGit": false,
      "yes": true
    },
    "run": {
      "stream": true
    }
  },
  "loglevel": "error",
  "npmClient": "yarn",
  "npmClientArgs": [
    "--quiet"
  ],
  "useWorkspaces": true,
  "lerna": "2.11.0",
  "packages": [
    "interfaces/*",
    "packages/*",
    "services/*"
  ],
  "sort": true,
  "version": "0.1.0"
}
